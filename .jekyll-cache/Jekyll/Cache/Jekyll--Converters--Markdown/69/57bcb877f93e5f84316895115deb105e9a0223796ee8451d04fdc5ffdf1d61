<script src="/styleguide/js/libs/list.min.js"></script>

<script>
  // Create the List
  var options = {
    valueNames: [ 'header', 'group', 'styles', { name: 'maturity', attr: 'data-maturity' } ]
  };
  var roadmap = new List('sg_roadmap', options);
  roadmap.sort("header", {
    order: "asc"
  })
</script>

